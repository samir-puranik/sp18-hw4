<h2>Cats</h2>
<% @all_cats.each do |t| %>
        <strong>Name: <%= t.name %></strong><br>
        <br>
  <% end %>

<h2>Users</h2>
<% @all_users.each do |t| %>
        <strong>Username: <%= t.username %></strong><br>
        <strong>Email: <%= t.email %></strong><br>
        <strong>Age: <%= t.age %></strong><br>
        <br>
  <% end %>

<h2>Todos</h2>
<% @all_todos.each do |t| %>
        <strong>Task: <%= t.tasks %></strong><br>
        <strong>Email: <%= t.finished %></strong><br>
        <br>
  <% end %>

<%= link_to("Make a new todo", create_todos_path) %>
